<script>
	$(".icon").click(function(){
		$(".widget").slideUp();
		$("#twitter").slideDown().html('<img src="/assets/twitter.png" />');
		$("#twitter").css("height", "1200px");
		$("#twitter").css("width", "1200px");
		$("#twitter").css("overflow", "hidden");
	});
	$(".widget").click(function(){
		$(".widget").slideUp();
		$(this).slideDown().html('<img src="/assets/twitter.png" />');
		$(this).css("height", "1200px");
		$(this).css("width", "1200px");
		$(this).css("overflow", "hidden");
	});
</script>
